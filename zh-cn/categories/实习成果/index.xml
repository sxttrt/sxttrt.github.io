&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;Axis1:&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;MC_AXIS_REF&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;          &lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;轴引用变量&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;Position:&lt;/span&gt; &lt;span class=&#34;nv&#34;&gt;LREAL&lt;/span&gt; &lt;span class=&#34;o&#34;&gt;:=&lt;/span&gt; &lt;span class=&#34;m&#34;&gt;500.0&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;    &lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;目标位置（单位：&lt;/span&gt;&lt;span class=&#34;nv&#34;&gt;mm&lt;/span&gt;&lt;span class=&#34;err&#34;&gt;）&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;Velocity:&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;LREAL&lt;/span&gt; &lt;span class=&#34;o&#34;&gt;:=&lt;/span&gt; &lt;span class=&#34;m&#34;&gt;100.0&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;    &lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;运动速度（单位：&lt;/span&gt;&lt;span class=&#34;nv&#34;&gt;mm&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;/&lt;/span&gt;&lt;span class=&#34;nv&#34;&gt;s&lt;/span&gt;&lt;span class=&#34;err&#34;&gt;）&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;Acceleration:&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;LREAL&lt;/span&gt; &lt;span class=&#34;o&#34;&gt;:=&lt;/span&gt; &lt;span class=&#34;m&#34;&gt;500.0&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;加速度（单位：&lt;/span&gt;&lt;span class=&#34;nv&#34;&gt;mm&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;/&lt;/span&gt;&lt;span class=&#34;nv&#34;&gt;s&lt;/span&gt;&lt;span class=&#34;err&#34;&gt;²）&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;Deceleration:&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;LREAL&lt;/span&gt; &lt;span class=&#34;o&#34;&gt;:=&lt;/span&gt; &lt;span class=&#34;m&#34;&gt;500.0&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;减速度（单位：&lt;/span&gt;&lt;span class=&#34;nv&#34;&gt;mm&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;/&lt;/span&gt;&lt;span class=&#34;nv&#34;&gt;s&lt;/span&gt;&lt;span class=&#34;err&#34;&gt;²）&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;Start_Sig:&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;BOOL&lt;/span&gt; &lt;span class=&#34;o&#34;&gt;:=&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;FALSE&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;    &lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;启动信号（&lt;/span&gt;&lt;span class=&#34;nc&#34;&gt;DI输入&lt;/span&gt;&lt;span class=&#34;err&#34;&gt;）&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;Done_Sig:&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;BOOL&lt;/span&gt; &lt;span class=&#34;o&#34;&gt;:=&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;FALSE&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;    &lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;完成信号（&lt;/span&gt;&lt;span class=&#34;nc&#34;&gt;DO输出&lt;/span&gt;&lt;span class=&#34;err&#34;&gt;）&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;&lt;span class=&#34;nf&#34;&gt;END_VAR&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;&lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;轴使能与回零&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;&lt;span class=&#34;nc&#34;&gt;IF&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;Start_Sig&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;AND&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;NOT&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;Axis1&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;.&lt;/span&gt;&lt;span class=&#34;nc&#34;&gt;Status&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;.&lt;/span&gt;&lt;span class=&#34;nc&#34;&gt;Enabled&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;THEN&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;MC_Enable&lt;/span&gt;(&lt;span class=&#34;nc&#34;&gt;Axis1&lt;/span&gt;)&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;           &lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;轴使能函数块&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nc&#34;&gt;MC_Home&lt;/span&gt;(&lt;span class=&#34;nc&#34;&gt;Axis1&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;,&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;TRUE&lt;/span&gt;)&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;       &lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;轴回零函数块&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;&lt;span class=&#34;nf&#34;&gt;END_IF&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;&lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;nv&#34;&gt;绝对位置运动&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;&lt;span class=&#34;nc&#34;&gt;IF&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;Axis1&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;.&lt;/span&gt;&lt;span class=&#34;nc&#34;&gt;Status&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;.&lt;/span&gt;&lt;span class=&#34;nc&#34;&gt;Homed&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;AND&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;NOT&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;Done_Sig&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;THEN&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;MC_MoveAbsolute&lt;/span&gt;(&lt;span class=&#34;nc&#34;&gt;Axis1&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;,&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;Position&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;,&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;Velocity&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;,&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;Acceleration&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;,&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;Deceleration&lt;/span&gt;&lt;span class=&#34;nf&#34;&gt;,&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;FALSE&lt;/span&gt;)&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nf&#34;&gt;IF&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;Axis1&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;.&lt;/span&gt;&lt;span class=&#34;nc&#34;&gt;Status&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;.&lt;/span&gt;&lt;span class=&#34;nc&#34;&gt;OperationComplete&lt;/span&gt; &lt;span class=&#34;nf&#34;&gt;THEN&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;    &lt;span class=&#34;nf&#34;&gt;Done_Sig&lt;/span&gt; &lt;span class=&#34;o&#34;&gt;:=&lt;/span&gt; &lt;span class=&#34;nc&#34;&gt;TRUE&lt;/span&gt;&lt;span class=&#34;p&#34;&gt;;&lt;/span&gt;         &lt;span class=&#34;nf&#34;&gt;//&lt;/span&gt; &lt;span class=&#34;err&#34;&gt;运动完成置位&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;  &lt;span class=&#34;nc&#34;&gt;END_IF&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;&lt;span class=&#34;nf&#34;&gt;END_IF&lt;/span&gt;
&lt;/span&gt;&lt;/span&gt;&lt;/code&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;
